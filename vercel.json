{
  "version": 2,
  "builds": [
    {
      "src": "platform/frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://YOUR-RAILWAY-BACKEND-URL.up.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/platform/frontend/$1"
    }
  ]
}
